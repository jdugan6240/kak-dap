{
    "configurations": {
        "test_gdb": {
            "adapter": "vscode-cpptools",
            "launch_args": {
                "program": "${CUR_DIR}/test_c",
                "args": [],
                "cwd": "${CUR_DIR}",
                "externalConsole": false,
                "MIMode": "gdb",
                "logging": {
                    "programOutput": false
                },
                "stopAtEntry": true,
                "setupCommands": [
                    {
                        "text": "set startup-with-shell off"
                    },
                    {
                        "text": "-enable-pretty-printing"
                    }
                ]
            }
        },
        "test_lldb": {
            "adapter": "vscode-cpptools",
            "launch_args": {
                "program": "${CUR_DIR}/test_c",
                "args": [],
                "cwd": "${CUR_DIR}",
                "externalConsole": false,
                "MIMode": "lldb",
                "logging": {
                    "programOutput": false
                },
                "stopAtEntry": true
            }
        }
    }
}
