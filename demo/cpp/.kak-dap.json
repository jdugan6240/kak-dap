  "launch_args": {
    "program": "${CUR_DIR}/test_c",
    "args": [],
    "cwd": "${CUR_DIR}",
    "externalConsole": false,
    "environment": [],
    "MIMode": "gdb",
    "logging": {
        "programOutput": false
    }
  }
}
